{
	"name": "Pastelefull",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"enumMember": {
		  "foreground": "#56b6c2"
		},
		"variable.constant": {
		  "foreground": "#d19a66"
		},
		"variable.defaultLibrary": {
		  "foreground": "#e5c07b"
		}
	},
	"tokenColors":[
		//CSS
		{
			"name": "Style",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#8DC994"
			}
		},
		{
			"name": "Prop",
			"scope": [
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#79ADDC"
			}
		},
		{
			"name": "Val",
			"scope": [
				"support.constant.property-value.css",
				"constant.numeric.css",
				"keyword.other.unit.px.css"
			],
			"settings": {
				"foreground": "#DFB656"
			}
		},
		{
			"name": "Val Mod",
			"scope": [
				"keyword.other.unit.px.css",
				"keyword.other.unit.em.css",
				"keyword.other.unit.vh.css",
				"keyword.other.unit.vw.css",
				"keyword.other.unit.cm.css",
				"keyword.other.unit.mm.css",
				"keyword.other.unit.in.css",
				"keyword.other.unit.pt.css",
				"keyword.other.unit.pc.css",
				"keyword.other.unit.ex.css",
				"keyword.other.unit.ch.css",
				"keyword.other.unit.rem.css",
				"keyword.other.unit.vmin.css",
				"keyword.other.unit.vmax.css",
				"keyword.other.unit.%.css"
			],
			"settings": {
				"foreground": "#FFAC81"
			}
		},

		//Javascript
		{
			"name": "Function",
			"scope": [
				"storage.type.function.js"
			],
			"settings": {
				"foreground": "#BDB4F3"
			}
		},

		// HTML
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#79ADDC",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#BDB2FF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Atributes/Props",
			"scope": [
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#BDB2FF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Atributes/Props Values",
			"scope": [
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#F8F8FF9D"
			}
		},
		{
			"name": "Raw",
			"scope": [
				"text.html.derivative"
			],
			"settings": {
				"foreground": "#F8F8FF9D"
			}
		},

		//Json
		{
			"name": "Props",
			"scope": [
				"support.type.property-name.json",
				"punctuation.support.type.property-name.begin.json",
				"punctuation.support.type.property-name.end.json"
			],
			"settings": {
				"foreground": "#79ADDC",
				"fontStyle": "bold"
			},
		},
		{
			"name": "Props Quotes",
			"scope": [
				"punctuation.support.type.property-name.begin.json",
				"punctuation.support.type.property-name.end.json"
			],
			"settings": {
				"foreground": "#3d7eb8",
				"fontStyle": "bold"
			},
		},
		{
			"name": "Vals",
			"scope": [
				"string.quoted.double.json",
				"constant.numeric.json"
			],
			"settings": {
				"foreground": "#dfb656",
				"fontStyle": "bold"
			},
		},

		//C# and Default
		{
			"name": "Comments Start",
			"scope": [
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#df463b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Comments Text",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#ee6055"
			}
		},
		{
			"scope":"keyword.other.this",
			"settings": {
				"foreground": "#FF9B85",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String Text",
			"scope": [
				"string.quoted"
			],
			"settings": {
				"foreground": "#FFD97D"
			}
		},
		{
			"name": "String Text Quotes",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#ffac81"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#BDB2FF"
			}
		},
		{
			"name": "Textlike",
			"scope": [
				"entity.name.type.namespace",
				"keyword.other.set",
				"keyword.other.get",
				"sorce"
			],
			"settings": {
				"foreground": "#f8f8ff9d"
			}
		},
		{
			"name": "Opperators and Pointers etc",
			"scope": [
				"keyword.control.loop",
				"keyword.control.conditional",
				"constant.language",
				"keyword.control.flow",
				"keyword.operator",
				"keyword.other.await"
			],
			"settings": {
				"foreground": "#FFD97D"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#79ADDC"
			}
		},
		{
			"name": "Variables - 1st",
			"scope": [
				"variable.other.object",
				"entity.name.variable",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#79ADDC"
			}
		},
		{
			"name": "Variables - Next",
			"scope": [
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#bfcdda"
			}
		},
		{
			"name": "Namespace, Using, Sorage Modifier, Class Keyword, Keyword Type, Struct, New",
			"scope": [
				"keyword.other.using",
				"storage.modifier",
				"keyword.other.class",
				"keyword.type",
				"keyword.other.struct",
				"keyword.other.new",
				"keyword.other.namespace"
			],
			"settings": {
				"foreground": "#bdb4f3"
			}
		},
		{
			"name": "Entity Class",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#FF9B85",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage Types",
			"scope": [
				"storage.type",
				"keyword.other.var"
			],
			"settings": {
				"foreground": "#8dc994"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#da94da"
			}
		}
	],
	
